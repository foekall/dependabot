{
    "name": "gbmf-provider-client-portal",
    "version": "7.0.0",
    "scripts": {
      "ng": "ng",
      "prebuild": "rm -rf config.json && rm -rf dist/* && gulp",
      "start": "gulp && npm run write-environment -- --environment=local && ng serve",
      "build": "npm run write-environment -- --environment=deployment && ng build",
      "test": "gulp && npm run write-environment -- --environment=local && ng test",
      "test-headless": "gulp && npm run write-environment -- --environment=local && ng test --watch=false --browsers=ChromeHeadless",
      "coverage": "ng test --no-watch --code-coverage",
      "test:single-run": "gulp && ng test --watch=false",
      "lint": "eslint -c .eslintrc --ext .ts src",
      "e2e": "gulp && ng e2e",
      "decryptAppConfig": "ts-node utils/app.config.decryptor.ts",
      "encryptAppConfig": "ts-node utils/app.config.encryptor.ts",
      "write-environment": "ts-node utils/environment.writer.ts",
      "preinstall": "npx npm-force-resolutions"
    },
